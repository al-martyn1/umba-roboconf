(----------- UART RX/TX )

(generator "umba_periph_config" interface "UART:RX"
    (match "function" to "US{0,1}ART(\d{1,2})_RX"                 
        (modify "NetName" replaceSingle "(.*)_UART(\d*)(.*)"  "$({1})$({3})")
        (modify "NetName" replaceSingle "(.*)_RX$"  "$({1})")
        (set ( ("$(NET_NAME)_UART"                           to "UMBA_MCU_PERIPH_UART$({1})")
               ("$(NET_NAME)_UART_SPL"                       to "UART$({1})"         )
               ("$(NET_NAME)_LEGACY_UART"                    to "uart::uart$({1})"   )
               ("USE_UART$({1})"                             to "1" )
             )
        )
    )
    (match "function" to "P(A|B|C|D|E|F|G|H|I|J)(\d{1,2})"  
        (modify "NetName" replaceSingle "(.*)_UART(\d*)(.*)"  "$({1})$({3})")
        (modify "NetName" replaceSingle "(.*)_RX$"  "$({1})")
        (set ( ("$(NET_NAME)_UART_RX_GPIO"                   to "UMBA_MCU_PERIPH_GPIO$({1})")
               ("$(NET_NAME)_UART_RX_GPIO_SPL"               to "GPIO$({1})")
               ("$(NET_NAME)_UART_RX_GPIO_PIN_NO"            to "$({2})")
               ("$(NET_NAME)_UART_RX_GPIO_PIN_ADDR"          to "UMBA_MCU_PERIPH_PINADDR_P$({1})$({2})")
               ("$(NET_NAME)_UART_RX_GPIO_PORT_PIN_ADDR"     to "UMBA_MCU_PERIPH_PORT_PINADDR_P$({1})$({2})")
               ("$(NET_NAME)_UART_RX_GPIO_PIN_SOURCE_SPL"    to "GPIO_PinSource$({2})")
               ("$(NET_NAME)_UART_RX_GPIO_PIN_DIRECTION"     to "UMBA_MCU_PERIPH_GPIO_DIRECTION_IN")
             )
        )
    )
)

(generator "umba_periph_config" interface "UART:TX"
    (match "function" to "US{0,1}ART(\d{1,2})_TX"                 
        (modify "NetName" replaceSingle "(.*)_UART(\d*)(.*)"  "$({1})$({3})")
