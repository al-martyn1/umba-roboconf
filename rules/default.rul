        (modify "NetName" replaceSingle "(.*)_MOSI$"         "$({1})")
        (modify "NetName" replaceSingle "(.*)_MISO$"         "$({1})")
        (modify "NetName" replaceSingle "^SCK_(.*)"          "$({1})")
        (modify "NetName" replaceSingle "^SPC_(.*)"          "$({1})")
