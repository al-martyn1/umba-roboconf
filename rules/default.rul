(-- knownInterfaces "UART" "SPI" "I2C" "PWM" "DAC" "ADC" "GPIO")

(-- match "token"    - lookup on mcu net tokens )
(-- match "function" - lookup on target pin functions )

(-- detect UART RX/TX - duplex)
(detect "UART"
    (and (match "token" (any "TXD{0,1}") set "TX" )
         (match "token" (any "RXD{0,1}") set "RX" )
    )
)

(-- detect UART RX/TX only - single direction)
(detect "UART"
    (or  (match "token" (any "TXD{0,1}") set "TX" )
         (match "token" (any "RXD{0,1}") set "RX" )
    )
)

(detect "CAN"
    (and (match "token" (any "RX") set "RX" )
         (match "token" (any "TX") set "TX" )
         (match "token" (any "LBK" "LOOPBACK") set "GPIO:OUT" )
    )
)

(detect "CAN"
    (and (match "function" (any "RXD{0,1}") set "RX" )
         (match "function" (any "TXD{0,1}") set "TX" )
         (match "function" (any "LBK" "LOOPBACK") set "GPIO:OUT" )
    )
)

(-- detect I2C by MCU net)
(detect "I2C"
    (and (match "token" (any "SCL") set "SCL" )
         (match "token" (any "SDA") set "SDA" )
    )
)

(-- detect SPI by MCU net)
(detect "SPI"
    (and (match "token" (any "MOSI") set "MOSI" )
         (match "token" (any "MISO") set "MISO" )
         (match "token" (any "SCLK" "CLK" "SPC" "SCK") set "SCK" )
    )
)

(detect "SPI"
    (and (match "token" (any "MISO") set "MISO" )
         (match "token" (any "SCLK" "CLK" "SPC" "SCK") set "SCK" )
    )
)

(detect "SPI"
    (and (match "token" (any "MOSI") set "MOSI" )
         (match "token" (any "SCLK" "CLK" "SPC" "SCK") set "SCK" )
    )
)

(-- detect I2C by target unit pin function)
(-- detect "I2C"
    (and (match "function" (any "SCL") set "SCL" )
         (match "function" (any "SDA") set "SDA" )
    )
)

(-- detect SPI by target unit pin function)
(-- detect "SPI"
    (and (match "function" (any "MOSI") set "MOSI" )
         (match "function" (any "MISO") set "MISO" )
         (match "function" (any "SCLK" "CLK" "SPC" "SCK") set "SCK" )
    )
)

(-- detect "SPI"
    (and (match "function" (any "MISO") set "MISO" )
         (match "function" (any "SCLK" "CLK" "SPC" "SCK") set "SCK" )
    )
)

(--detect "SPI"
    (and (match "function" (any "MOSI") set "MOSI" )
         (match "function" (any "SCLK" "CLK" "SPC" "SCK") set "SCK" )
    )
)


(detect "ADC"
    (and (match "function" (any "ADC") set "ADC" )
    )
)

(detect "DAC"
    (and (match "function" (any "DAC") set "DAC" )
    )
)

(detect "PWM"
    (and (match "token"   (any "PWM")    set "PWM:" )
    )
)

(detect "PWM"
    (and (match "function" (any "PWM") set "PWM:" )
    )
)



(detect "HBRIDGE"
    (or (match "function" (any "DAC") set "DAC:DAC" )
    )
)



(-- ----------------- Configuration geneartor - Umba ----------------- )


(generatorInfo (name "umba_periph_config")
               (description "C-header config generator")
               (defaultFileExtention "h")
               (lineComment "//")
               (commentStart "/*")
               (commentEnd "*/")
               (lineFormat "#define $(name)    $(value)")
               (nameWidth  "42")
)


(-- match token|netToken|function|pinFunction|purpose|unitPurpose)
(generator "umba_periph_config" interface "GPIO:OUT"
    (match "function" to "P(A|B|C|D|E|F|G|H|I|J)(\d{1,2})"
        (set ( ("$(NET_NAME)_GPIO"                 to "UMBA_MCU_PERIPH_GPIO$({1})")
               ("$(NET_NAME)_GPIO_SPL"             to "GPIO$({1})")
               ("$(NET_NAME)_GPIO_PIN_NO"          to "$({2})")
               ("$(NET_NAME)_GPIO_PIN_ADDR"        to "UMBA_MCU_PERIPH_PINADDR_P$({1})$({2})")
               ("$(NET_NAME)_GPIO_PORT_PIN_ADDR"   to "UMBA_MCU_PERIPH_PORT_PINADDR_P$({1})$({2})")
               ("$(NET_NAME)_GPIO_PIN_ADDR_DIR"    to "UMBA_MCU_PERIPH_PINADDR_P$({1})$({2}), UMBA_MCU_PERIPH_GPIO_DIRECTION_OUT")
               ("$(NET_NAME)_GPIO_PIN_SOURCE_SPL"  to "GPIO_PinSource$({2})")
               ("$(NET_NAME)_GPIO_PIN_DIRECTION"   to "UMBA_MCU_PERIPH_GPIO_DIRECTION_OUT")
             )
        )
    )
)

(generator "umba_periph_config" interface "GPIO:IN"
    (match "function" to "P(A|B|C|D|E|F|G|H|I|J)(\d{1,2})"
        (set ( ("$(NET_NAME)_GPIO"                 to "UMBA_MCU_PERIPH_GPIO$({1})")
               ("$(NET_NAME)_GPIO_SPL"             to "GPIO$({1})")
               ("$(NET_NAME)_GPIO_PIN_NO"          to "$({2})")
               ("$(NET_NAME)_GPIO_PIN_ADDR"        to "UMBA_MCU_PERIPH_PINADDR_P$({1})$({2})")
               ("$(NET_NAME)_GPIO_PORT_PIN_ADDR"   to "UMBA_MCU_PERIPH_PORT_PINADDR_P$({1})$({2})")
               ("$(NET_NAME)_GPIO_PIN_ADDR_DIR"    to "UMBA_MCU_PERIPH_PINADDR_P$({1})$({2}), UMBA_MCU_PERIPH_GPIO_DIRECTION_IN")
               ("$(NET_NAME)_GPIO_PIN_SOURCE_SPL"  to "GPIO_PinSource$({2})")
               ("$(NET_NAME)_GPIO_PIN_DIRECTION"   to "UMBA_MCU_PERIPH_GPIO_DIRECTION_IN")
             )
        )
    )
)

(generator "umba_periph_config" interface "ADC:.*"
    (match "function" to "P(A|B|C|D|E|F|G|H|I|J)(\d{1,2})"  
        (set ( ("$(NET_NAME)_ADC_GPIO"                to "UMBA_MCU_PERIPH_GPIO$({1})")
               ("$(NET_NAME)_ADC_GPIO_SPL"            to "GPIO$({1})")
               ("$(NET_NAME)_ADC_GPIO_PIN_NO"         to "$({2})")
               ("$(NET_NAME)_ADC_GPIO_PIN_ADDR"       to "UMBA_MCU_PERIPH_PINADDR_P$({1})$({2})")
               ("$(NET_NAME)_ADC_GPIO_PORT_PIN_ADDR"  to "UMBA_MCU_PERIPH_PORT_PINADDR_P$({1})$({2})")
               ("$(NET_NAME)_ADC_GPIO_PIN_SOURCE_SPL" to "GPIO_PinSource$({2})")
             )
        )
    )
    (match "function" to "ADC(\d{1,3})_IN(\d{1,2})"
        (set ( ("$(NET_NAME)_ADC_CHANNEL"          to "UMBA_MCU_PERIPH_ADC_CHANNEL_$({2})")
               ("$(NET_NAME)_ADC_CHANNEL_SPL"      to "ADC_Channel_$({2})")
               ("$(NET_NAME)_ADC_CHANNEL_NO"       to "$({2})")
               ("$(NET_NAME)_ADC"                  to "UMBA_MCU_PERIPH_ADC$({1})" (-- comment "Informational only") )
               ("$(NET_NAME)_ADC_SPL"              to "ADC$({1})" (-- comment "Informational only") )
               ("USE_ADC$({1})"                    to "1" )
             )
        )
    )
)

(generator "umba_periph_config" interface "DAC:.*"
    (match "function" to "DAC(\d{1,3})_OUT(\d{1,1})"
        (set ( ("$(NET_NAME)_DAC"                to "UMBA_MCU_PERIPH_DAC$({1})")
               ("$(NET_NAME)_DAC_SPL"            to "DAC$({1})")
               ("$(NET_NAME)_DAC_CHANNEL"        to "UMBA_MCU_PERIPH_DAC_CHANNEL_$({2})")
               ("$(NET_NAME)_DAC_CHANNEL_SPL"    to "DAC_Channel_$({2})")
               ("$(NET_NAME)_DAC_CHANNEL_NO"     to "$({2})")
               ("USE_DAC$({1})"                  to "1" )
             )
        )
    )
    (match "function" to "DAC(\d{1,3})_OUT"
        (set ( ("$(NET_NAME)_DAC"                to "UMBA_MCU_PERIPH_DAC")
               ("$(NET_NAME)_DAC_SPL"            to "DAC")
               ("$(NET_NAME)_DAC_CHANNEL"        to "UMBA_MCU_PERIPH_DAC_CHANNEL_$({1})")
               ("$(NET_NAME)_DAC_CHANNEL_SPL"    to "DAC_Channel_$({1})")
               ("USE_DAC"                        to "1" )
             )
        )
    )
)



